{
  "name": "amazeing",
  "version": "0.0.0-development",
  "description": "Visualises different algorithms tailored to creating and traversing mazes.",
  "homepage": "https://amazething.netlify.app/",
  "repository": "https://github.com/Ijee/Amazeing",
  "license": "MIT",
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/ijeee"
  },
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:prod-ci": "node build-version.js && node build-articles.js && ng build --configuration production",
    "source-map": "source-map-explorer dist/amazeing/browser/**/*.js",
    "prettier": "prettier --write .",
    "test": "ng test",
    "test:pw": "playwright test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky",
    "release": "semantic-release",
    "release-dry-run": "semantic-release --dry-run"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "20.2.10",
    "@angular/common": "21.0.2",
    "@angular/compiler": "21.0.2",
    "@angular/core": "21.0.2",
    "@angular/forms": "21.0.2",
    "@angular/platform-browser": "21.0.2",
    "@angular/platform-browser-dynamic": "21.0.2",
    "@angular/router": "21.0.2",
    "@angular/service-worker": "21.0.2",
    "@fortawesome/angular-fontawesome": "4.0.0",
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-brands-svg-icons": "7.1.0",
    "@fortawesome/free-regular-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "angular-eslint": "^21.0.1",
    "animejs": "^4.2.2",
    "bulma": "1.0.4",
    "driver.js": "1.3.6",
    "file-saver": "2.0.5",
    "lodash-es": "4.17.21",
    "pako": "2.1.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "typescript-eslint": "8.46.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "21.0.1",
    "@angular/cli": "21.0.1",
    "@angular/compiler-cli": "21.0.2",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@playwright/test": "1.56.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@types/jasmine": "5.1.12",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.8.1",
    "dotenv": "^16.4.7",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "jasmine-core": "5.12.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "16.2.4",
    "marked": "16.4.1",
    "prettier": "3.6.2",
    "protractor": "7.0.0",
    "semantic-release": "25.0.0",
    "source-map-explorer": "2.5.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1"
  },
  "engines": {
    "node": "^24.10.0"
  },
  "lint-staged": {
    "*.{js,ts,html}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "release": {
    "branches": [
      "main",
      "semantic-release"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "preset": "angular"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features",
                "hidden": false
              },
              {
                "type": "fix",
                "section": "Bug Fixes",
                "hidden": false
              },
              {
                "type": "perf",
                "section": "Performance Improvements",
                "hidden": false
              },
              {
                "type": "refactor",
                "section": "Code Refactoring",
                "hidden": false
              },
              {
                "type": "build",
                "section": "Build System",
                "hidden": false
              },
              {
                "type": "chore",
                "section": "Build System",
                "hidden": true
              },
              {
                "type": "ci",
                "section": "Continuous Integration",
                "hidden": false
              },
              {
                "type": "docs",
                "section": "Documentation",
                "hidden": true
              },
              {
                "type": "style",
                "section": "Styles",
                "hidden": true
              },
              {
                "type": "test",
                "section": "Tests",
                "hidden": true
              }
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "subject",
              "scope"
            ]
          }
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  }
}
