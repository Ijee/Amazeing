{
  "name": "amazeing",
  "version": "0.0.0-development",
  "description": "Visualises different algorithms tailored to creating and traversing mazes.",
  "homepage": "https://amazething.netlify.app/",
  "repository": "https://github.com/Ijee/Amazeing",
  "license": "MIT",
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/ijeee"
  },
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:prod-ci": "node build-version.js && node build-articles.js && ng build --configuration production",
    "source-map": "source-map-explorer dist/amazeing/browser/**/*.js",
    "prettier": "prettier --write .",
    "test": "ng test",
    "test:pw": "playwright test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky",
    "release": "semantic-release",
    "release-dry-run": "semantic-release --dry-run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.5",
    "@angular/cdk": "^19.2.8",
    "@angular/common": "^19.2.5",
    "@angular/compiler": "^19.2.5",
    "@angular/core": "^19.2.5",
    "@angular/forms": "^19.2.5",
    "@angular/platform-browser": "^19.2.5",
    "@angular/platform-browser-dynamic": "^19.2.5",
    "@angular/router": "^19.2.5",
    "@angular/service-worker": "^19.2.5",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "angular-eslint": "^19.3.0",
    "bulma": "^1.0.3",
    "driver.js": "^1.3.5",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.11.1",
    "lodash-es": "^4.17.21",
    "pako": "^2.1.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.2.0",
    "typescript-eslint": "^8.29.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular/cli": "^19.2.6",
    "@angular/compiler-cli": "^19.2.5",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@playwright/test": "^1.51.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jasmine": "^5.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "angular-eslint": "19.3.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.11",
    "husky": "^9.1.7",
    "jasmine-core": "^5.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "15.5.0",
    "prettier": "^3.5.3",
    "protractor": "~7.0.0",
    "semantic-release": "^24.2.3",
    "source-map-explorer": "^2.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.27.0"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "lint-staged": {
    "*.{js,ts,html}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "release": {
    "branches": [
      "main",
      "semantic-release"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "preset": "angular"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features",
                "hidden": false
              },
              {
                "type": "fix",
                "section": "Bug Fixes",
                "hidden": false
              },
              {
                "type": "perf",
                "section": "Performance Improvements",
                "hidden": false
              },
              {
                "type": "refactor",
                "section": "Code Refactoring",
                "hidden": false
              },
              {
                "type": "build",
                "section": "Build System",
                "hidden": false
              },
              {
                "type": "chore",
                "section": "Build System",
                "hidden": true
              },
              {
                "type": "ci",
                "section": "Continuous Integration",
                "hidden": false
              },
              {
                "type": "docs",
                "section": "Documentation",
                "hidden": true
              },
              {
                "type": "style",
                "section": "Styles",
                "hidden": true
              },
              {
                "type": "test",
                "section": "Tests",
                "hidden": true
              }
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "subject",
              "scope"
            ]
          }
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  }
}
